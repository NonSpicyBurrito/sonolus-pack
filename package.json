    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.13",
    "@sonolus/core": "~7.11.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0"
  }
