{
  "name": "@sonolus/pack",
  "version": "5.4.6",
  "description": "CLI tool to pack Sonolus source files into repository and database",
  "author": "NonSpicyBurrito",
  "repository": "github:Sonolus/sonolus-pack",
  "license": "MIT",
  "keywords": [
    "Sonolus"
  ],
  "bin": {
